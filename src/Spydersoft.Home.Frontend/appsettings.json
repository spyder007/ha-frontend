{
  "AllowedHosts": "*",
  "Identity": {
    "Audience": "unifi.ipmanager",
    "Authority": "https://auth.mattgerega.net"
  },
  "Logging": {
    "OpenTelemetry": {
      "IncludeFormattedMessage": true,
      "IncludeScopes": true,
      "ParseStateValues": true
    }
  },
  "Serilog": {
    "MinimumLevel": "Warning"
  },
  "Telemetry": {
    "ActivitySourceName": "HA.Frontend.Activity",
    "Log": {
      "Otlp": {
        "Protocol": "http"
      },
      "Type": "otlp"
    },
    "MeterName": "HA.Frontend.Meter",
    "Metrics": {
      "Otlp": {
        "Protocol": "http"
      },
      "Type": "otlp"
    },
    "ServiceName": "HA.Frontend",
    "Trace": {
      "Type": "otlp"
    }
  },
  "YarpSettings": {
    "Clusters": {
      "techRadarApi": {
        "Destinations": {
          "techRadarApi/destination1": {
            "Address": "http://techradar-data-api/"
          }
        }
      }
    },
    "Routes": {
      "data": {
        "AllowAnonymous": false,
        "ClusterId": "techRadarApi",
        "Match": {
          "Path": "/api/data/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/api/data"
          }
        ]
      }
    }
  }
}
